<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
	"http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
	
<urlrewrite>

	<!-- this is a rule where we map from /products/nnn to an url that is a 
		jsp page with the product id as a parameter -->
	<rule>
		<from>/start/products/([0-9]+)</from>
		<to>/end/products/index.jsp?product_id=$1</to>
	</rule>
</urlrewrite>